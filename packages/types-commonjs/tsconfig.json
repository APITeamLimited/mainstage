{
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./dist",
    "allowJs": true,
    "esModuleInterop": true,
    "target": "esnext",
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "src-copied/*": [
        "./src-copied/*",
      ],
    },
    "skipLibCheck": true,
    "declaration": true
  }
}
