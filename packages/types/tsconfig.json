{
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./dist",
    "allowJs": true,
    "esModuleInterop": true,
    "target": "esnext",
    "strict": true,
    "module": "ES2022",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "./src/*",
      ],
    },
    "skipLibCheck": true,
    "declaration": true
  }
}
